  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:migrations": "jest test/migrations.test.ts",
    "prepare": "npx husky",
    "migration:generate": "npx typeorm-ts-node-commonjs -d typeorm.config.ts migration:generate src/database/migrations/$npm_config_name",
    "migration:run": "npx typeorm-ts-node-commonjs -d typeorm.config.ts migration:run",
    "migration:revert": "npx typeorm-ts-node-commonjs -d typeorm.config.ts migration:revert",
    "migration:show": "npx typeorm-ts-node-commonjs -d typeorm.config.ts migration:show",
    "migration:generate:legacy": "ts-node -r tsconfig-paths/register -r reflect-metadata src/cli/migration-generate.ts",
    "migration:run:legacy": "ts-node -r tsconfig-paths/register -r reflect-metadata src/cli/migration-run.ts",
    "migration:revert:legacy": "ts-node -r tsconfig-paths/register -r reflect-metadata src/cli/migration-revert.ts",
    "migration:status:legacy": "ts-node -r tsconfig-paths/register -r reflect-metadata src/cli/migration-status.ts",
    "seed": "npx ts-node src/database/seeds/seed.ts",
    "seed:run": "ts-node -r tsconfig-paths/register src/database/seeds/database.seeder.ts",
    "seed:status": "ts-node -r tsconfig-paths/register src/cli/seed-status.ts",
    "db:setup": "npm run migration:run && npm run seed",
    "db:reset": "npm run migration:revert && npm run migration:run && npm run seed"
  },
